{
  "name": "ng-svg-icon",
  "version": "1.0.1",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ng-svg-icon -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start",
    "publish-sample-app": "rm -rf sampleApp/ && cp -rf ./sample/ng-svg-icon-sample/www/ sampleApp/ && git add sampleApp && git commit -m 'Sample App Update' && git subtree push --prefix sampleApp origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasanchit90/ng-svg-icon"
  },
  "author": {
    "name": "Sanchit Gupta",
    "email": "gupta.sanchit90@gmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasanchit90/ng-svg-icon/issues"
  },
  "devDependencies": {
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/compiler-cli": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@compodoc/compodoc": "1.1.9",
    "@types/jasmine": "3.3.13",
    "@types/node": "12.0.7",
    "angular-in-memory-web-api": "0.8.0",
    "codelyzer": "5.1.0",
    "concurrently": "4.1.0",
    "core-js": "3.1.3",
    "del": "4.1.1",
    "gulp": "4.0.2",
    "gulp-rename": "1.4.0",
    "gulp-rollup": "2.16.2",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lite-server": "2.5.3",
    "node-sass": "4.12.0",
    "node-sass-tilde-importer": "1.0.2",
    "node-watch": "0.6.2",
    "protractor": "5.4.2",
    "rollup": "1.14.3",
    "run-sequence": "2.2.1",
    "rxjs": "6.5.2",
    "systemjs": "3.1.6",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "typescript": "3.5.1",
    "zone.js": "0.9.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}